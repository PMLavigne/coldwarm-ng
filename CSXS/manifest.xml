<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ExtensionManifest
    xmlns="ExtensionManifest"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="ExtensionManifest https://raw.githubusercontent.com/Adobe-CEP/CEP-Resources/master/CEP_6.x/ExtensionManifest_v_5_0.xsd"
    ExtensionBundleId="codes.patrick.coldwarm-ng"
    ExtensionBundleName="Coldwarm NG"
    ExtensionBundleVersion="0.0.1"
    Version="5.0">
  <ExtensionList>
    <Extension Id="com.example.coldwarm-ng.picker" Version="0.0.1"/>
  </ExtensionList>
  <ExecutionEnvironment>
    <HostList>
      <Host Name="PHXS" Version="14"/>
    </HostList>
    <LocaleList>
      <Locale Code="All"/>
    </LocaleList>
    <RequiredRuntimeList>
      <RequiredRuntime Name="CSXS" Version="4.0"/>
    </RequiredRuntimeList>
  </ExecutionEnvironment>
  <DispatchInfoList>
    <Extension Id="com.example.coldwarm-ng.picker">
      <DispatchInfo>
        <Resources>
          <MainPath>./index.html</MainPath>
        </Resources>
        <UI>
          <Type>Panel</Type>
          <Menu>ColdWarm NG</Menu>
          <Geometry>
            <Size>
              <Height>400</Height>
              <Width>400</Width>
            </Size>
          </Geometry>
        </UI>
      </DispatchInfo>
    </Extension>
  </DispatchInfoList>
</ExtensionManifest>
